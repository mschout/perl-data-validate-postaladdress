{
   "fields" : [
      "name",
      "organization",
      "street_address",
      "city",
      "zip"
   ],
   "format" : "{{$name}}%n{{$organization}}%n{{$street_address}}%n{{$city}} PR {{$zip}}",
   "postal_code_prefix" : "PR ",
