      "street_address",
      "city",
      "zip"
   ],
   "uppercase_fields" : [
      "street_address",
      "city",
      "name",
      "organization"
   ],
   "zip_pattern" : "(00[679]\\d{2})(?:[ \\-](\\d{4}))?"
}
